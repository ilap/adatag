    "buffer": "^6.0.3",
    "flowbite-react": "^0.8.0",
    "koios-tiny-client": "^1.1.0",
    "node-fetch": "^2.7.0",
    "react-confetti": "^6.1.0",
    "stream-json": "^1.8.0"
  },
  "devDependencies": {
    "@meshsdk/react": "^1.1.10-beta.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/rollup-darwin-x64": "^4.9.6",
    "@sqlite.org/sqlite-wasm": "github:sqlite/sqlite-wasm#node",
