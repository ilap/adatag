    "cors-anywhere": "^0.4.4",
    "eslint": "^8.55.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-wasm": "^3.3.0"
