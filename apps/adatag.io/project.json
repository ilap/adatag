{
  "name": "@adatag/adatag.io",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/adatag.io/src",
  "projectType": "application",
  "outputPath": "../../dist/apps/adatag.io/",
  "tags": [],
  "implicitDependencies": [
    "@adatag/deploy",
    "!@adatag/common",
    "!@adatag/integri-tree"
  ],
  "// targets": "to see all targets run: nx show project adatag1.io --web",
  "targets": {
    "build": {
      "dependsOn": ["^deploy"],
      "inputs": ["production"]
    },
    "serve-static": {
      "configurations": {
        "custom": {}
      },
      "inputs": [
        "!{projectRoot}/public/genesis-config.json"
      ]
    }
  }
}